package cn.showclear.www.pojo.base;

import java.sql.Time;

/**
 * @author Wang Junbo
 * @date 2019/4/8
 * 出售形式实体类，对应数据库出售形式表T_SALE_WAY
 */
public class SaleWayDo {
    private Integer saleWayId;
    private String code;
    private String name;
    private Time timeLimit;

    public Integer getSaleWayId() {
        return saleWayId;
    }

    public void setSaleWayId(Integer saleWayId) {
        this.saleWayId = saleWayId;
    }

    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public Time getTimeLimit() {
        return timeLimit;
    }

    public void setTimeLimit(Time timeLimit) {
        this.timeLimit = timeLimit;
    }

    @Override
    public String toString() {
        return "SaleWayDo{" +
                "saleWayId=" + saleWayId +
                ", code='" + code + '\'' +
                ", name='" + name + '\'' +
                ", timeLimit=" + timeLimit +
                '}';
    }
}
