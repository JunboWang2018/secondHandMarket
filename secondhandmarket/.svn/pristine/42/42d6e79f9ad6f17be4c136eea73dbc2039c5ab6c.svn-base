package cn.showclear.www.pojo.base;

import java.sql.Timestamp;

/**
 * @author Wang Junbo
 * @date 2019/4/8
 * 议价记录表实体类
 */
public class BargainingRecordDo {
    private Integer bargainingRecordId;
    private Integer productId;
    private Double bargaringPrice;
    private String description;
    private Integer userId;
    private Timestamp createDate;

    public Integer getBargainingRecordId() {
        return bargainingRecordId;
    }

    public void setBargainingRecordId(Integer bargainingRecordId) {
        this.bargainingRecordId = bargainingRecordId;
    }

    public Integer getProductId() {
        return productId;
    }

    public void setProductId(Integer productId) {
        this.productId = productId;
    }

    public Double getBargaringPrice() {
        return bargaringPrice;
    }

    public void setBargaringPrice(Double bargaringPrice) {
        this.bargaringPrice = bargaringPrice;
    }

    public String getDescription() {
        return description;
    }

    public void setDescription(String description) {
        this.description = description;
    }

    public Integer getUserId() {
        return userId;
    }

    public void setUserId(Integer userId) {
        this.userId = userId;
    }

    public Timestamp getCreateDate() {
        return createDate;
    }

    public void setCreateDate(Timestamp createDate) {
        this.createDate = createDate;
    }

    @Override
    public String toString() {
        return "BargainingRecordDo{" +
                "bargainingRecordId=" + bargainingRecordId +
                ", productId=" + productId +
                ", bargaringPrice=" + bargaringPrice +
                ", description='" + description + '\'' +
                ", userId=" + userId +
                ", createDate=" + createDate +
                '}';
    }
}
